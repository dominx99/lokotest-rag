# Production Environment
# CORS configuration for production - regex pattern
# Matches: https://lokotest.pl and https://anything.lokotest.pl
CORS_ORIGIN_REGEX=^https://(.*\.)?lokotest\.pl$

# OpenAI Configuration
OPENAI_API_KEY=your_production_openai_api_key_here

# RAG Configuration
RAG_CHAT_MODEL=gpt-4o-mini
RAG_TOPK=5
RAG_CHARS_PER_HIT=500

# Qdrant Configuration
QDRANT_URL=https://your-production-qdrant-url
QDRANT_API_KEY=your_production_qdrant_api_key
QDRANT_COLLECTION=rag_documents

# Embedding Configuration
EMB_MODEL=text-embedding-3-small
RERANKER=jinaai/jina-reranker-v2-base-multilingual

# Search Parameters
VEC_TOPK=100
BM25_TOPK=150
MERGE_TOPK=40
FINAL_K=8
RRF_K=60

# Logging
LOG_LEVEL=INFO