# Local Development Environment
# CORS configuration for local development - regex pattern
# Matches: http(s)://anything.docker.localhost with optional port OR localhost with port
CORS_ORIGIN_REGEX=^https?://((.*\.)?docker\.localhost(:[0-9]+)?|localhost(:[0-9]+)?)$

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# RAG Configuration
RAG_CHAT_MODEL=gpt-4o-mini
RAG_TOPK=5
RAG_CHARS_PER_HIT=500

# Qdrant Configuration
QDRANT_URL=https://your-qdrant-url
QDRANT_API_KEY=your_qdrant_api_key
QDRANT_COLLECTION=rag_documents

# Embedding Configuration
EMB_MODEL=text-embedding-3-small
RERANKER=jinaai/jina-reranker-v2-base-multilingual

# Search Parameters
VEC_TOPK=100
BM25_TOPK=150
MERGE_TOPK=40
FINAL_K=8
RRF_K=60

# Logging
LOG_LEVEL=WARNING